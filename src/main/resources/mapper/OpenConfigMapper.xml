<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ren.ascence.mapper.OpenConfigMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="ren.ascence.entity.OpenConfig">
		<id column="id" property="id" />
		<result column="token" property="token" />
		<result column="encoding_aes_key" property="encodingAesKey" />
		<result column="app_id" property="appId" />
		<result column="app_secret" property="appSecret" />
		<result column="host" property="host" />
		<result column="mapphost" property="mapphost" />
		<result column="mini_username" property="miniUsername" />
		<result column="status" property="status" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, token, encoding_aes_key AS encodingAesKey, app_id AS appId, app_secret AS appSecret, host, mapphost, mini_username AS miniUsername, status
    </sql>

</mapper>
